{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/analyze-roof.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/data-layer.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/geocode.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/calculate-roi.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/calculate-roi",
      "dest": "/api/calculate-roi.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}

